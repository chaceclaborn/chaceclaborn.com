{
  "name": "chaceclaborn-portfolio",
  "version": "2.0.0",
  "description": "Professional portfolio website with Firebase integration",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview --open",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "deploy": "yarn build && aws s3 sync dist/ s3://chaceclaborn.com --delete",
    "deploy:dryrun": "yarn build && aws s3 sync dist/ s3://chaceclaborn.com --delete --dryrun",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,html,css,json}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,html,css,json}'",
    "check": "yarn test:run && yarn lint && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaceclaborn/chaceclaborn.com.git"
  },
  "keywords": [
    "portfolio",
    "firebase",
    "aws",
    "web-development",
    "full-stack"
  ],
  "author": "Chace Claborn",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/ui": "^1.2.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.56.0",
    "jsdom": "^23.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.4",
    "tailwindcss": "^4.1.17",
    "vite": "^5.4.19",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "firebase": "^10.7.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}

{
  "name": "chaceclaborn-portfolio",
  "version": "2.0.0",
  "description": "Professional portfolio website with Firebase integration",
  "main": "index.html",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "deploy": "aws s3 sync . s3://chaceclaborn.com --exclude '.git/*' --exclude '.gitignore' --exclude '*.bat' --exclude 'dev/*' --exclude 'node_modules/*' --delete",
    "serve": "python -m http.server 8000",
    "lint": "eslint js/**/*.js",
    "format": "prettier --write '**/*.{js,html,css,json}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaceclaborn/chaceclaborn.com.git"
  },
  "keywords": [
    "portfolio",
    "firebase",
    "aws",
    "web-development"
  ],
  "author": "Chace Claborn",
  "license": "MIT",
  "devDependencies": {
    "@vitest/ui": "^1.2.0",
    "eslint": "^8.56.0",
    "jsdom": "^23.2.0",
    "prettier": "^3.2.4",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "firebase": "^10.7.1"
  }
}